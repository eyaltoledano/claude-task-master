# API Keys (Required to enable respective provider)
ANTHROPIC_API_KEY=your_anthropic_api_key_here       # Required: Format: sk-ant-api03-...
PERPLEXITY_API_KEY=your_perplexity_api_key_here     # Optional: Format: pplx-...
OPENAI_API_KEY=your_openai_api_key_here             # Optional, for OpenAI/OpenRouter models. Format: sk-proj-...
GOOGLE_API_KEY=your_google_api_key_here             # Optional, for Google Gemini models.
MISTRAL_API_KEY=your_mistral_key_here               # Optional, for Mistral AI models.
XAI_API_KEY=YOUR_XAI_KEY_HERE                       # Optional, for xAI AI models.
AZURE_OPENAI_API_KEY=your_azure_key_here            # Optional, for Azure OpenAI models (requires endpoint in .taskmasterconfig).

# Provider override settings (optional)
# These override the automatic provider detection based on model name
AI_PROVIDER=custom                                  # Global provider override for all roles
AI_PROVIDER_MAIN=custom                             # Provider override for main role
AI_PROVIDER_RESEARCH=custom                         # Provider override for research role
AI_PROVIDER_FALLBACK=custom                         # Provider override for fallback role

# Custom provider settings
CUSTOM_AI_API_KEY=your_custom_api_key_here          # Optional, for custom OpenAI-compatible providers.
CUSTOM_AI_API_BASE_URL=https://your-api-endpoint.com/v1 # Optional, base URL for custom OpenAI-compatible provider.
CUSTOM_AI_MODEL=your_model_name                     # Optional, model ID for custom OpenAI-compatible provider.
# Role-specific models for custom provider (falls back to CUSTOM_AI_MODEL if not set)
CUSTOM_AI_MODEL_MAIN=your_main_model_name           # Optional, model ID for main role with custom provider.
CUSTOM_AI_MODEL_RESEARCH=your_research_model_name   # Optional, model ID for research role with custom provider.
CUSTOM_AI_MODEL_FALLBACK=your_fallback_model_name   # Optional, model ID for fallback role with custom provider.
CUSTOM_AI_HEADERS={"header1":"value1","header2":"value2"} # Optional, JSON string of custom headers for API requests.
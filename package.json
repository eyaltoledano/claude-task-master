{
	"name": "task-master-ai",
	"version": "0.23.1-rc.0",
	"description": "A task management system for ambitious AI-driven development that doesn't overwhelm and confuse Cursor.",
	"main": "index.js",
	"type": "module",
	"bin": {
		"task-master": "bin/task-master.js",
		"task-master-mcp": "mcp-server/server.js",
		"task-master-ai": "mcp-server/server.js"
	},
	"workspaces": ["apps/*", "."],
	"scripts": {
		"test": "node --experimental-vm-modules node_modules/.bin/jest",
		"test:fails": "node --experimental-vm-modules node_modules/.bin/jest --onlyFailures",
		"test:watch": "node --experimental-vm-modules node_modules/.bin/jest --watch",
		"test:coverage": "node --experimental-vm-modules node_modules/.bin/jest --coverage",
		"test:e2e": "./tests/e2e/run_e2e.sh",
		"test:e2e-report": "./tests/e2e/run_e2e.sh --analyze-log",
		"prepare": "chmod +x bin/task-master.js mcp-server/server.js",
		"changeset": "changeset",
		"release": "changeset publish",
		"inspector": "npx @modelcontextprotocol/inspector node mcp-server/server.js",
		"mcp-server": "node mcp-server/server.js",
		"format-check": "biome format .",
		"format": "biome format . --write",
		"db:generate": "drizzle-kit generate --config=scripts/modules/flow/drizzle.config.ts",
		"db:generate:remote": "node scripts/modules/flow/scripts/db-generate-remote.js",
		"db:migrate": "drizzle-kit migrate --config=scripts/modules/flow/drizzle.config.ts",
		"db:migrate:remote": "node scripts/modules/flow/scripts/db-migrate-remote.js",
		"db:push": "drizzle-kit push --config=scripts/modules/flow/drizzle.config.ts",
		"db:push:remote": "node scripts/modules/flow/scripts/db-push-remote.js",
		"db:reset": "node scripts/modules/flow/scripts/db-reset.js",
		"db:reset:remote": "node scripts/modules/flow/scripts/db-reset-remote.js",
		"db:nuke": "node scripts/modules/flow/scripts/db-nuke.js",
		"db:nuke:remote": "node scripts/modules/flow/scripts/db-nuke.js",
		"db:setup": "node scripts/modules/flow/scripts/db-setup.js",
		"db:setup:remote": "node scripts/modules/flow/scripts/db-setup-remote.js",
		"db:studio": "drizzle-kit studio --config=scripts/modules/flow/drizzle.config.ts",
		"db:studio:remote": "node scripts/modules/flow/scripts/db-studio-remote.js"
	},
	"keywords": [
		"claude",
		"task",
		"management",
		"ai",
		"development",
		"cursor",
		"anthropic",
		"llm",
		"mcp",
		"context"
	],
	"author": "Eyal Toledano",
	"license": "MIT WITH Commons-Clause",

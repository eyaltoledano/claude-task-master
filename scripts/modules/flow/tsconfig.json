{
	"compilerOptions": {
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "node",
		"target": "ES2022",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"strict": false,
		"noEmit": true
	},
	"include": ["**/*.jsx", "**/*.js"]
}

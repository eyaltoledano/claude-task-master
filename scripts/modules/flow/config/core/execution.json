{
  "timeout": 300000,
  "maxRetries": 2,
  "streamOutput": true,
  "parallelTasks": 3,
  "environment": {
    "nodeEnv": "development",
    "preserveEnv": true,
    "additionalEnvVars": {}
  },
  "resources": {
    "maxMemoryMB": 1024,
    "maxCpuPercent": 80,
    "diskQuotaMB": 5000
  },
  "safety": {
    "allowNetworkAccess": true,
    "allowFileSystemAccess": true,
    "allowProcessSpawning": true,
    "blockedCommands": [],
    "allowedFileExtensions": [".js", ".ts", ".py", ".go", ".json", ".md", ".txt"]
  },
  "monitoring": {
    "enabled": true,
    "logLevel": "info",
    "trackPerformance": true,
    "alertOnFailure": true
  },
  "caching": {
    "enabled": true,
    "maxCacheSize": "100MB",
    "ttl": 3600000
  }
} 
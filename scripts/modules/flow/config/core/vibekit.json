{
  "enabled": true,
  "defaultAgent": "claude-code",
  "streamingEnabled": true,
  "githubIntegration": true,
  "autoCreatePR": false,
  "sdk": {
    "version": "latest",
    "timeout": 120000,
    "retryAttempts": 3
  },
  "telemetry": {
    "enabled": false,
    "samplingRate": 0.1,
    "batchSize": 100,
    "flushInterval": 30000,
    "serviceName": "taskmaster-flow",
    "serviceVersion": "1.0.0"
  },
  "sessionManagement": {
    "enabled": true,
    "persistSessions": true,
    "sessionDir": ".taskmaster/flow/sessions",
    "maxSessionAge": 604800000,
    "cleanupInterval": 86400000,
    "autoResume": true
  },
  "secrets": {
    "enabled": true,
    "provider": "env"
  },
  "askMode": {
    "enabled": true,
    "defaultMode": "interactive",
    "timeout": 60000,
    "maxQuestions": 10
  },
  "streaming": {
    "enabled": true,
    "bufferSize": 1000,
    "flushInterval": 500,
    "compression": false
  }
} 
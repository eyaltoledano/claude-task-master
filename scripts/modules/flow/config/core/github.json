{
  "enabled": true,
  "autoDetectRepo": true,
  "defaultBranch": "main",
  "autoSync": false,
  "branchPrefix": "taskmaster/",
  "integration": {
    "enabled": true,
    "autoCreatePR": false,
    "autoMerge": false,
    "requireReviews": true,
    "deleteSourceBranch": false
  },
  "pullRequest": {
    "assignToCreator": true,
    "addLabels": ["taskmaster", "automated"],
    "requestReviewers": [],
    "draft": false
  },
  "commit": {
    "messageTemplate": "[TaskMaster] {task_title}",
    "signCommits": false,
    "author": {}
  },
  "webhooks": {
    "enabled": false,
    "events": ["push", "pull_request"]
  }
} 